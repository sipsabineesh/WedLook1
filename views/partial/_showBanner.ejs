<% for (var i=0; i < banner.length;i++) {%>
    <tr>
        <td><%= i+1%></td>
        <td><%= banner[i].bannerTitle%></td>
        <td><%= banner[i].bannerLink%></td>
        <td>
             <img src="/img/banner-images/<%= banner[i].bannerImage[0]%>" width="50px" height="50px"></td>
       <td>
            <!-- <a class="btn action-btn border-shadow delete delete-banner" data-id="<%= banner[i]._id%>">
                <span class="text-gradient"><i class="fas fa-times"></i></span>
            </a> -->
            <% if(banner[i].isActive === true) {%>
                <a  href=""  id="<%= banner[i]._id%>"  class="btn action-btn border-shadow user-unblocked">
                    <span class="text-gradient"><i class="fa-solid fa-lock-open"></i></span>
                </a>
            <%} else {%>
                <a href=""  id="<%= banner[i]._id%>"  class="btn action-btn border-shadow user-blocked">
                    <span class="text-gradient"><i class="fa-solid fa-lock"></i></span>
                </a>
            <% } %>
        </td>
    </tr>
<%}%>